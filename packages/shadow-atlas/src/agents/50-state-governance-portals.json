{
  "metadata": {
    "created": "2025-11-25",
    "purpose": "State Governance District Discovery - Legislative, Congressional, County-Level",
    "scope": "All 50 states + DC for state legislative, congressional, and county commission districts",
    "sources": [
      "Census Bureau TIGER/Line (authoritative congressional districts)",
      "State GIS clearinghouses (from registry/state-gis-portals.ts)",
      "State redistricting commission websites (where available)"
    ],
    "notes": "Focus on STATE-LEVEL governance, NOT city council (already have 3,413 city_council layers)"
  },
  "census_tiger_base": {
    "congressional_districts": {
      "source": "US Census Bureau TIGER/Line",
      "url_pattern": "https://www2.census.gov/geo/tiger/TIGER2023/CD/",
      "coverage": "All 50 states + DC",
      "format": "Shapefile",
      "notes": "Authoritative source, updated every 2 years after redistricting",
      "phase": 3,
      "reasoning": "Phase 3 dependency - requires shapefile processing pipeline"
    },
    "state_legislative_upper": {
      "source": "US Census Bureau TIGER/Line",
      "url_pattern": "https://www2.census.gov/geo/tiger/TIGER2023/SLDU/",
      "coverage": "All 50 states (49 with upper chambers, NE unicameral)",
      "format": "Shapefile",
      "notes": "State senate districts",
      "phase": 3
    },
    "state_legislative_lower": {
      "source": "US Census Bureau TIGER/Line",
      "url_pattern": "https://www2.census.gov/geo/tiger/TIGER2023/SLDL/",
      "coverage": "All 50 states (49 with lower chambers, NE unicameral)",
      "format": "Shapefile",
      "notes": "State house/assembly districts",
      "phase": 3
    }
  },
