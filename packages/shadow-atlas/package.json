{
    "name": "@voter-protocol/shadow-atlas",
    "version": "0.1.0",
    "description": "Geospatial voting district registry with Merkle tree proofs for VOTER Protocol",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "test": "vitest",
        "test:watch": "vitest watch",
        "test:unit": "vitest run --config vitest.unit.config.ts",
        "test:integration": "RUN_INTEGRATION=true vitest run --config vitest.integration.config.ts",
        "test:e2e": "RUN_E2E=true vitest run --config vitest.e2e.config.ts",
        "test:e2e:tiger": "RUN_E2E=true vitest run src/__tests__/e2e/real-tiger-pipeline.test.ts",
        "test:performance": "vitest run --config vitest.performance.config.ts",
        "test:nightly": "RUN_E2E=true RUN_INTEGRATION=true vitest run",
        "test:coverage": "vitest run --coverage",
        "discover:batch": "tsx src/scripts/batch-discover.ts",
        "discover:top100": "tsx src/scripts/batch-discover.ts --strategy=population --limit=100 --concurrency=20",
        "discover:state": "tsx src/scripts/batch-discover.ts --strategy=state --concurrency=10",
        "discover:staging": "tsx src/scripts/batch-discover.ts --staging --concurrency=100 --limit=1000",
        "dashboard": "tsx src/scripts/coverage-dashboard.ts",
        "gaps": "tsx -e \"import {analyzeCoverage} from './src/services/coverage-analyzer.js'; const TOP_50=[{fips:'3651000',name:'NYC',state:'NY',population:8804190}]; const stats=await analyzeCoverage(TOP_50); console.log(JSON.stringify(stats.topGaps,null,2));\"",
        "stale": "tsx -e \"import {getStaleData} from './src/services/coverage-analyzer.js'; const stale=await getStaleData(90); console.log(JSON.stringify(stale,null,2));\"",
        "retry:worker": "tsx src/scripts/retry-worker.ts",
        "retry:stats": "tsx src/scripts/query-retry-candidates.ts stats",
        "retry:candidates": "tsx src/scripts/query-retry-candidates.ts candidates",
        "retry:blocker": "tsx src/scripts/query-retry-candidates.ts blocker",
        "validate:registry": "tsx src/scripts/validate-registry.ts",
        "freshness:check": "tsx src/scripts/check-freshness.ts",
        "freshness:revalidate": "tsx src/scripts/check-freshness.ts --revalidate",
        "plan:expansion": "tsx src/scripts/plan-expansion.ts",
        "plan:100": "tsx src/scripts/plan-expansion.ts 100",
        "plan:1000": "tsx src/scripts/plan-expansion.ts 1000",
        "analyze:batch-results": "tsx src/scripts/analyze-batch-results.ts",
        "merge:staging": "tsx src/scripts/merge-staging-worker.ts",
        "merge:once": "tsx -e \"import {mergeStagingToShards} from './src/scripts/merge-staging-worker.js'; await mergeStagingToShards();\"",
        "extract:vtd": "tsx src/scripts/extract-vtd.ts",
        "extract:vtd:force": "tsx src/scripts/extract-vtd.ts --force",
        "validate:geoids": "tsx src/scripts/validate-all-geoids.ts",
        "validate:geoids:verbose": "tsx src/scripts/validate-all-geoids.ts --verbose",
        "validate:tiger": "tsx src/scripts/validate-tiger-geoids.ts",
        "validate:cross": "tsx src/scripts/cross-validate.ts",
        "validate:cross:verbose": "tsx src/scripts/cross-validate.ts --verbose",
        "audit:freshness": "tsx src/scripts/freshness-audit.ts",
        "audit:freshness:verbose": "tsx src/scripts/freshness-audit.ts --verbose",
        "report:comprehensive": "tsx src/scripts/comprehensive-report.ts",
        "report:comprehensive:full": "tsx src/scripts/comprehensive-report.ts --full",
        "report:comprehensive:json": "tsx src/scripts/comprehensive-report.ts --json"
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.0.0",
        "@noble/hashes": "^1.3.3",
        "@turf/boolean-point-in-polygon": "^7.2.0",
        "@turf/helpers": "^7.2.0",
        "@turf/turf": "^7.2.0",
        "better-sqlite3": "^11.10.0",
        "dotenv": "^16.4.7",
        "jszip": "^3.10.1",
        "multiformats": "^13.4.2",
        "pako": "^2.1.0",
        "shapefile": "^0.6.6",
        "zod": "^3.24.1"
    },
    "peerDependencies": {
        "@aztec/bb.js": "^2.1.8",
        "@noir-lang/noir_js": "^1.0.0-beta.16",
        "@voter-protocol/crypto": "^0.1.0"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.11",
        "@types/node": "^24.3.2",
        "@types/pako": "^2.0.4",
        "tsx": "^4.7.0",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "keywords": [
        "voter-protocol",
        "geospatial",
        "districts",
        "merkle-tree",
        "zk-proofs",
        "census",
        "tiger"
    ],
    "author": "VOTER Protocol",
    "license": "MIT"
}
